{
  "name": "ore-hq-pool-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:prod": "ng build --configuration production",
    "build:quality": "ng build --configuration=quality --stats-json",
    "build:test": "ng build --configuration=test",
    "build:install": "npm ci --ignore-scripts",
    "build:release": "ng lint --fix && ng build --configuration production",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ore-hq-pool-web/stats.json",
    "generateAPI": "rm -rf ./src/app/api && openapi-generator-cli generate -g typescript-angular -i ./openapi.json -o ./src/app/api",
    "code:lint": "ng lint --fix",
    "code:prettier": "npx prettier -w --ignore-unknown",
    "code:quality": "ng lint --fix && npx prettier -w --ignore-unknown",
    "husky:start": "husky install",
    "prepare": "husky install",
    "check-stats": "webpack-bundle-analyzer ./dist/ore-hq-pool-web/stats.json",
    "format": "prettier ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@ngneat/transloco": "^6.0.4",
    "@ngrx/signals": "^17.2.0",
    "angular-oauth2-oidc": "^17.0.2",
    "bootstrap": "^5.3.3",
    "date-fns": "^3.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular-eslint/eslint-plugin-template": "^17.3.0",
    "@angular-eslint/schematics": "^17.3.0",
    "@angular-eslint/template-parser": "^17.3.0",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^43.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "ng-packagr": "^17.3.0",
    "prettier": "^3.2.5",
    "prettier-config-standard": "^7.0.0",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "typescript": "~5.4.2"
  }
}
